{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "1.0.0",
  "description": "__MSG_extDescription__",
  "default_locale": "zh_CN",
  "permissions": [
    "storage",
    "unlimitedStorage",
    "bookmarks"
  ],
  "host_permissions": [
    "https://*.bing.com/*",
    "https://api.github.com/*",
    "https://*.zhihu.com/*",
    "https://*.v2ex.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "src/newtab/index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["src/newtab/*", "assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}